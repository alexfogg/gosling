.nivoSlider {
    position:relative;
    background:url(<%= asset_path "loading.gif" %>) no-repeat 50% 50%;
}
.nivoSlider img {
    position:absolute;
    top:0px;
    left:0px;
    display:none;
}
.nivoSlider a {
    border:0;
    display:block;
}


li {
  list-style: none;
}

.title{
  color: white;
  font-size: 30px;
}

h1{
  font-size: 60px;
}

#container{
width: 800px;
max-height: 750px;
overflow: scroll;
background: url('conback.png');
background-repeat: repeat;
margin: 20px auto;
padding: 40px 40px;
-moz-box-shadow: inset 0 0 10px 10px #888;
-webkit-box-shadow: inset 0 0 10px 10px#888;
box-shadow: inset 0 0 10px 10px #888;
border-radius: 40px;
opacity: 0.9;
}

#goslingfeed table {
  background-color: transparent;
  border: 0px;
}

#goslingfeed a {
  color: black;
}

#goslingfeed .button {
  background-color: grey;
  border: 1px black;
}